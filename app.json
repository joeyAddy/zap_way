{
  "expo": {
    "name": "Zap Way",
    "slug": "zap_way",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F80ED"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.joeydevs.zapway",
      "config": {
        "googleMapsApiKey": "AIzaSyBBBqxCpYXUdQCsngCusR-OVu8q-cFVLUU"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["INTERNET"],
      "package": "com.joeydevs.zap_way",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCdwXiHqzyahxsXtOTzz6ooJ34uDaae95E"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://zap_way.dev/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://zap_way.dev/"
      },
      "eas": {
        "projectId": "992dea11-c42f-4380-bfef-c124a7ee5408"
      }
    }
  }
}
